        "02": { "$type": "dimension", "$value": "{core.size.04}" },
        "03": { "$type": "dimension", "$value": "{core.size.06}" },
        "full": { "$type": "dimension", "$value": "1920px" }
      }
    },
    "elevation": {
      "default": {
        "$type": "shadow",
        "$value": [
          {
            "x": "0px",
            "y": "calc({semantic.elevation.depth} * 0.25px)",
            "blur": "calc({semantic.elevation.depth} * 0.75px)",
            "spread": "0px",
            "color": "rgba(20, 20, 20, {core.opacity.100})",
            "type": "dropShadow"
          },
          {
            "x": "0px",
            "y": "calc({semantic.elevation.depth} * 0.125px)",
            "blur": "calc({semantic.elevation.depth} * 0.25px)",
            "spread": "0px",
            "color": "rgba(20, 20, 20, {core.opacity.50})",
            "type": "dropShadow"
          }
        ]
