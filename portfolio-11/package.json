{
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@open-wc/building-rollup": "^1.10.0",
    "@open-wc/eslint-config": "^0.3.12",
    "@rollup/plugin-url": "^5.0.1",
    "@web/dev-server": "^0.1.17",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.8.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^7.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.1",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-copy-assets": "^2.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "serialize-javascript": ">=4.0.0"
  },
  "scripts": {
    "lint": "eslint --ext .js,.html . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format": "eslint --ext .js,.html . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore",
    "build": "rimraf dist && rollup -c rollup.config.js",
    "start:build": "npm run build && web-dev-server --root-dir dist --app-index index.html --open --compatibility none",
    "deploy": "npm run build && gh-pages -d dist -t ",
    "start": "wds --node-resolve --app-index index.html --watch --open"
  },
  "name": "portfolio-11",
  "version": "0.0.0",
  "description": "Webcomponent portfolio-11 following open-wc recommendations",
  "author": "portfolio-11",
  "license": "MIT",
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@power-elements/stripe-elements": "^2.3.8",
    "dotenv": "^10.0.0",
    "lit": "*",
    "lit-html": "^1.4.1",
    "lit-redux-router": "^0.18.0",
    "moment": "^2.29.1",
    "redux": "^4.1.0",
    "redux-thunk": "^2.3.0"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
